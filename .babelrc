{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "plugins": [
    "lodash",
    "transform-react-constant-elements",
    ["transform-react-remove-prop-types", {
      "mode": "wrap"
    }],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        ["__coverage__", { "ignore": "test/" }]
      ]
    }
  }
}
